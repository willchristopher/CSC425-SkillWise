    "cookie-parser": "^1.4.6",
    "pg": "^8.11.3",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "pino-pretty": "^10.2.3",
    "dotenv": "^16.3.1",
    "@sentry/node": "^7.81.1",
    "openai": "^4.20.1",
    "nodemailer": "^6.9.7",
